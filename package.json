{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "start": "concurrently  \"php artisan serve\" \"npm run hot\" \"npm run start-server\"",
        "start-server": "cd server && npm run start"
    },
    "devDependencies": {
        "axios": "^0.21.1",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "vue-loader": "^15.9.5",
        "vue-template-compiler": "^2.6.14"
    },
    "dependencies": {
        "concurrently": "^6.2.0",
        "redis-server": "^1.2.2",
        "socket.io-client": "^2.4.0",
        "vue": "^2.6.14",
        "vue-axios": "^3.2.4",
        "vue-router": "^3.5.1",
        "vue-socket.io": "^3.0.10",
        "vue-socket.io-extended": "^4.2.0",
        "vuex": "^3.6.2"
    }
}
